{
  "buildCommand": "pnpm run build",
  "devCommand": "pnpm run dev",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "nodeVersion": "18.17.x",
  "env": [
    {
      "key": "NEXT_PUBLIC_SUPABASE_URL",
      "value": "@supabase_url",
      "type": "string"
    },
    {
      "key": "NEXT_PUBLIC_SUPABASE_ANON_KEY",
      "value": "@supabase_anon_key",
      "type": "string"
    }
  ],
  "buildOutputDirectory": ".next",
  "publicSource": "./public",
  "cleanUrls": true,
  "skipDownload": false,
  "crons": []
}
